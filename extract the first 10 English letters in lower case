first_10_lower <- letters[1:10]
last_10_upper <- toupper(letters[17:26])
letters_22_to_24_upper <- toupper(letters[22:24])

cat("First 10 letters in lower case:\n", first_10_lower, "\n")
cat("Last 10 letters in upper case:\n", last_10_upper, "\n")
cat("Letters 22nd to 24th in upper case:\n", letters_22_to_24_upper, "\n")
